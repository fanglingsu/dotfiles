! cursortheme
Xcursor.theme: DMZ-White

! font settings
Xft.dpi:       96
Xft.autohint:  0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting:   1
Xft.antialias: 1
Xft.rgba:      rgb

urxvt.saveLines:         5000
urxvt.internalBorder:    0
urxvt.font:              xft:dejavu sans mono:normal:pixelsize=14
urxvt.boldFont:          xft:dejavu sans mono:bold:pixelsize=14
urxvt.letterSpace:       -1
urxvt.lineSpace:         1
urxvt.scrollBar:         false
urxvt.urgentOnBell:      true
urxvt.scrollTtyOutput:   false
urxvt.scrollWithBuffer:  true
urxvt.scrollTtyKeypress: true
urxvt.loginShell:        true
urxvt.transparent:       false

! set terminal colors
*cursorColor:  #B0B0B0
*pointerColor: #EEEEEE
urxvt.background:   #111111
urxvt.foreground:   #D3D7CF
! black
*color0:       #2E3436
*color8:       #555753
! red
*color1:       #CC0000
*color9:       #EF2929
! green
*color2:       #4E9A06
*color10:      #8AE234
! yellow
*color3:       #C4A000
*color11:      #FCE94F
! blue
*color4:       #3465A4
*color12:      #729FCF
! magenta
*color5:       #75507B
*color13:      #AD7FA8
! cyan
*color6:       #06989A
*color14:      #34E2E2
! white
*color7:       #D3D7CF
*color15:      #EEEEEC
